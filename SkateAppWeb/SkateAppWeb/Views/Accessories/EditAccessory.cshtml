@model SkateAppWeb.Models.Products.AccessoryCreateVM

@{
    ViewData["Title"] = "EditProduct";
}

<head>
    <link rel="stylesheet" href="~/css/createPages.css" asp-append-version="true" />
</head>
<h1 class="text-center createH">Edit Accessory</h1>

<div>
    <form asp-action="EditAccessory" class="text-center">
        @await Html.PartialAsync("_AccessoryPartialView", Model)
        <div class="form-group mt-4">
            <div class="form-group">
                <button type="submit" class="buttonCreate">Edit</button>
            </div>
            <a asp-action="AllTable" asp-controller="Products" class="linkCreate">Back to products</a>
        </div>
    </form>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}